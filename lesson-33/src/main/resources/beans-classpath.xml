<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

	<bean id="city" class="beans.City"></bean>
	<bean id="city-san-francisco"
		name="city-san-francisco-alias-1,city-san-francisco-alias-2; city-san-francisco-alias-3"
		class="beans.City">
		<constructor-arg value='San Francisco' name="name"/>
		<constructor-arg value="United States of America" name="countryName"/>
		<constructor-arg value="864816" name="population"/>
	</bean>
</beans>
